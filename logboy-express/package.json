{
  "name": "logboy-express",
  "version": "0.0.1",
  "description": "Log and Meetrics Transport Library for Node.js",
  "main": "build/cjs/index.js",
  "module": "build/esm/index.js",
  "types": "build/types/index.d.ts",
  "type": "module",
  "author": "harish-x",
  "exports": {
    ".": {
      "import": "./build/esm/index.js",
      "require": "./build/cjs/index.js",
      "types": "./build/types/index.d.ts"
    }
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:types": "tsc --project tsconfig.types.json",
    "build:copy": "node scripts/build-package.js",
    "build": "npm run build:esm && npm run build:cjs && npm run build:types && npm run build:copy"
  },
  "devDependencies": {
    "@babel/cli": "^7.27.2",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.27.1",
    "@babel/plugin-transform-runtime": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-typescript": "^7.27.1",
    "@babel/runtime": "^7.27.1",
    "@types/node": "^22.15.18",
    "core-js": "^3.42.0",
    "cross-env": "^7.0.3",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "vsts-npm-auth": "^0.43.0"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.13.3",
    "@grpc/proto-loader": "^0.7.15",
    "google-proto-files": "^4.2.0",
    "morgan": "^1.10.1",
    "winston": "^3.17.0",
    "winston-transport": "^4.9.0"
  }
}
